<h3><%= @weathers[0].location_name %></h3>
<table class="table">
	<thead>
      <tr>
        <th scope="col">性質</th>
        <th scope="col">日期</th>
        <th scope="col">狀態</th>
        <th scope="col">值</th>
      </tr>
    </thead>
	<tbody>
		<% @weathers.each do |weather| %>
			<tr>
	          <td><%= weather.element %></td>
	          <td><%= weather.time %></td>
	          <td><%= weather.parameter %></td>
	          <td><%= weather.parameter_value %></td>
			</tr>
		<% end %>
	</tbody>
</table>
